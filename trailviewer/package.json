{
    "name": "trailviewer",
    "version": "0.1.0",
    "main": "./dist/trailviewer.umd.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/trailviewer.es.mjs",
            "require": "./dist/trailviewer.umd.js"
        },
        "./dist/trailviewer.css": {
            "import": "./dist/trailviewer.css",
            "require": "./dist/trailviewer.css"
        }
    },
    "types": "./dist/trailviewer.d.ts",
    "scripts": {
        "build": "rimraf ./dist && rollup -c rollup.config.js --bundleConfigAsCjs",
        "format": "prettier --write .",
        "check": "prettier --check . && eslint ."
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.1",
        "@types/events": "^3.0.0",
        "@types/mapbox-gl": "^2.7.11",
        "@types/node": "^20.2.3",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "cheap-ruler": "^3.0.2",
        "eslint": "^8.41.0",
        "events": "^3.3.0",
        "mapbox-gl": "^2.14.1",
        "pannellum": "github:orosmatthew/pannellum-metroparks#trailview",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.1",
        "rollup": "^3.23.0",
        "rollup-plugin-import-css": "^3.2.1",
        "tslib": "^2.5.2",
        "typescript": "^5.0.4",
        "url-join": "^5.0.0"
    },
    "files": [
        "./dist/*"
    ]
}
