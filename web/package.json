{
	"name": "trailview-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && vite build",
		"post-clone": "npm run build-pannellum && npm run build-trailviewer && npm ci",
		"build-trailviewer": "cd ../trailviewer && npm ci && npm run build",
		"build-pannellum": "cd ../pannellum-metroparks/utils/build && ./build.sh",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte --write .",
		"start": "node --env-file=.env build",
		"pre-commit": "npm run format && npm run lint && npm run check"
	},
	"engines": {
		"node": "20"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/bootstrap": "^5.2.10",
		"@types/eslint": "^8.56.10",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/events": "^3.0.3",
		"@types/geojson": "^7946.0.14",
		"@types/geojson-vt": "^3.2.5",
		"@types/mapbox-gl": "^3.1.0",
		"@types/node": "^20.14.11",
		"@types/node-cron": "^3.0.11",
		"@types/uuid": "^10.0.0",
		"drizzle-kit": "^0.23.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.42.0",
		"npm-check-updates": "^16.14.20",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "^1.77.8",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.0.0-alpha.45",
		"vite": "^5.3.4"
	},
	"type": "module",
	"dependencies": {
		"@cmparks/pannellum": "file:../pannellum-metroparks",
		"@cmparks/trailviewer": "file:../trailviewer",
		"apexcharts": "^3.50.0",
		"bootstrap": "^5.3.3",
		"bootstrap-icons": "^1.11.3",
		"cheap-ruler": "^4.0.0",
		"drizzle-orm": "^0.32.0",
		"events": "^3.3.0",
		"geojson-vt": "^4.0.2",
		"iterative-closest-point": "^1.2.1",
		"mapbox-gl": "^3.5.2",
		"moment": "^2.30.1",
		"node-cron": "^3.0.3",
		"normalize.css": "^8.0.1",
		"pascal-case": "^3.1.2",
		"postgres": "^3.4.4",
		"simplify-js": "^1.2.4",
		"svelte-migrate": "^1.4.3",
		"url-join": "^5.0.0",
		"uuid": "^10.0.0",
		"vt-pbf": "^3.1.3",
		"zod": "^3.23.8"
	}
}
